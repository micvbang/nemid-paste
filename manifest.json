{
  "manifest_version": 3,

  "name": "NemID paste",
  "version": "1.2",
  "description": "Allow pasting in NemID applet",
  "content_scripts": [
    {
      "matches": ["https://applet.danid.dk/*"],
      "run_at": "document_idle",
      "all_frames": true,
      "js": ["let_me_paste.js"]
    }
  ]
}
